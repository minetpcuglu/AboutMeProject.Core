@model List<EducationDTO>
@{
    ViewData["Title"] = "GetList";
    Layout = "~/Views/Shared/_CoronaAdminLayout.cshtml";
}


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Education List</h4>
            <p class="card-description">
                Education <code>.table</code>
            </p>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>SchollName</th>
                            <th>Section</th>
                            <th>NoteAvarage</th>
                            <th>Date</th>
                            <th>Delete</th>
                            <th>Update</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.Id
                                </td>
                                <td class="budget">
                                    @item.SchollName
                                </td>
                                <td class="budget">
                                    @item.Section
                                </td>
                                <td class="budget">
                                    @item.NoteAvarage
                                </td>
                                <td class="budget">
                                    @item.Date
                                </td>
                                <td>
                                    <button type="button" class="btn btn-danger "
                                            onclick="DeleteEducations(@item.Id)">
                                        Delete
                                    </button>
                                </td>
                                @*<td><a href="/Skill/DeleteSkill/@item.Id" class="btn btn-warning"> <b>DELETE</b></a></td>*@
                                <td><a href="/Education/UpdateEducation/@item.Id" class="btn btn-warning"> <b>Update</b></a></td>

                            </tr>
                        }

                    </tbody>


                </table>
                <br />
                <a href="/Education/AddEducation/" class="btn btn-primary">New Add Education</a>
            </div>
        </div>
    </div>
</div>




@section scripts{
    <script type="text/javascript" src="~/Deletejs/Person.js"></script>
}


