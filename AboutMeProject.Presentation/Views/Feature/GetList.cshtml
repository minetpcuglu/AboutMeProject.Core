@model List<FeatureDTO>
@{
    ViewData["Title"] = "GetList";
    Layout = "~/Views/Shared/_CoronaAdminLayout.cshtml";
}


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Feature </h4>
            <p class="card-description">
                Feature <code>.table</code>
            </p>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Header</th>
                            <th>Name</th>
                            <th>Title</th>
                       
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @item.Id
                            </td>
                            <td class="budget">
                                @item.Haeder
                            </td>
                            <td class="budget">
                                @item.Name
                            </td>
                            <td class="budget">
                                @item.Title
                            </td>

                            <td><a href="/Feature/UpdateFeature/@item.Id" class="btn btn-warning"> <b>Update</b></a></td>

                        </tr>
                        }

                    </tbody>


                </table>
                <br />
                
            </div>
        </div>
    </div>
</div>



