@model List<ServiceDTO>
@{
    ViewData["Title"] = "GetList";
    Layout = "~/Views/Shared/_CoronaAdminLayout.cshtml";
}

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Setting List</h4>
                <p class="card-description">
                    Setting <code>.table</code>
                </p>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Title</th>
                                <th>Image</th>
                                <th>Delete</th>
                                <th>Update</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {


                                <tr>
                                    <td>@item.Id</td>

                                    <td>@item.Title</td>
                                    <td class="py-1">
                                        <img src="@item.ImageUrl" alt="image" />
                                    </td>
                                    <td>
                                        <button type="button" class="btn btn-danger "
                                                onclick="Delete(@item.Id)">
                                            Delete
                                        </button>
                                    </td>
                                    @*<td><a href="/Skill/DeleteSkill/@item.Id" class="btn btn-warning"> <b>DELETE</b></a></td>*@
                                    <td><a href="/Setting/UpdateSetting/@item.Id" class="btn btn-warning"> <b>Update</b></a></td>

                                </tr>
                            }

                        </tbody>
                      

                    </table>
                    <br />
                    <a href="/Setting/AddService/" class="btn btn-primary">New Add Service</a>
                </div>
            </div>
        </div>
    </div>





@section scripts{
    <script type="text/javascript" src="~/Deletejs/Setting.js"></script>
}



