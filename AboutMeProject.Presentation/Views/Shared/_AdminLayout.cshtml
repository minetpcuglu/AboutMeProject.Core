
<!DOCTYPE html>
<html>

<head>
    <partial name="~/Views/Shared/PartialViews/AdminPartialViews/HeaderPartialView.cshtml" />
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/jquery-validate/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <link href="~/Toastr/toastr.min.css" rel="stylesheet" />
    <script src="~/Toastr/toastr.min.js"></script>
    <script src="~/Toastr/toastr.min.js"></script>
    
</head>

<body>
    <!-- Sidenav -->
    <partial name="~/Views/Shared/PartialViews/AdminPartialViews/SideBarPartialView.cshtml" />
    <!-- Main content -->
    <div class="main-content" id="panel">
        <!-- Topnav -->
        <partial name="~/Views/Shared/PartialViews/AdminPartialViews/NavBarPartialView.cshtml" />
        <!-- Header -->
        <!-- Header -->

        <partial name="~/Views/Shared/PartialViews/AdminPartialViews/NavigationPartialView.cshtml" />
        <!-- Page content -->
        <div class="container-fluid mt--6">
            <div class="row">
                <div class="col">

                    @RenderBody()

                </div>
            </div>
            <br />
            <br />
            <!-- Footer -->
            <partial name="~/Views/Shared/PartialViews/AdminPartialViews/FooterPartialView.cshtml" />
        </div>
    </div>
    <!-- Argon Scripts -->
    <!-- Core -->
    <partial name="~/Views/Shared/PartialViews/AdminPartialViews/ScriptPartialView.cshtml" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>

